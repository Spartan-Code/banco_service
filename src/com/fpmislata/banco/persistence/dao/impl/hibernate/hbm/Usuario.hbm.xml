<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.fpmislata.banco.business.domain.Usuario" table="user" >
        <id  name="idUsuario" />
        <property  name="nombre" />
        <property column="password" name="passwordEncrypt" />
        <property name="email" />
        <property name="rol" />
    </class>
</hibernate-mapping>
